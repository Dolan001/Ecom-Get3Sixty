(){\n            this.$store.commit('setIsLoading', true)\n            await axios\n                .post('/api/v1/products/search/', {'query': this.query})\n                .then(res => {\n                    this.products = res.data\n                    console.log(this.products)\n                })\n                .catch(err => {\n                    console.log(err)\n                })\n            this.$store.commit('setIsLoading', false)\n        }\n    },\n}\n</script>\n\n<style>\n\n</style>"]}]}