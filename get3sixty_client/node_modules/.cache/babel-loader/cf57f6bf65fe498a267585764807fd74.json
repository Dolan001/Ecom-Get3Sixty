AAgBa,MAAhB,CAAuB,UAACC,GAAD,EAAMC,MAAN,EAAiB;AAC3C,eAAOD,GAAE,IAAKC,MAAM,CAACC,QAArB;AACH,OAFM,EAEL,CAFK,CAAP;AAGH,KALI;AAMLC,kBANK,4BAMW;AACZ,aAAO,KAAKlB,IAAL,CAAUC,KAAV,CAAgBa,MAAhB,CAAuB,UAACC,GAAD,EAAMC,MAAN,EAAiB;AAC3C,eAAOD,GAAE,IAAKC,MAAM,CAACN,OAAP,CAAeS,KAAf,GAAuBH,MAAM,CAACC,QAA5C;AACH,OAFM,EAEL,CAFK,CAAP;AAGJ;AAVK;AApBE,CAAf","names":["axios","CartItem","name","components","data","cart","items","mounted","$store","state","methods","removeFromCart","item","filter","i","product","id","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","price"],"sourceRoot":"","sources":["/Users/dolanbairagi/Public/Frameworks/PyJs/get3Sixty/get3sixty_client/src/views/Cart.vue"],"sourcesContent":["<template>\n  <div class=\"page-cart\">\n      <div class=\"columns is-multiline\">\n          <div class=\"column is-12\">\n              <h1 class=\"title\">Cart</h1>\n          </div>\n          <div class=\"column is-12 box\">\n              <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\n                  <thead>\n                      <tr>\n                          <th>Product</th>\n                          <th>Price</th>\n                          <th>Quantity</th>\n                          <th>Total</th>\n                          <th></th>\n                      </tr>\n                  </thead>\n\n                  <tbody>\n                      <CartItem \n                        v-for=\"item in cart.items\" \n                        :key=\"item.product.id\" \n                        :initialItem=\"item\"\n                        v-on:removeFromCart=\"removeFromCart\" />\n                  </tbody>\n              </table>\n              <p v-else>You don't have any product in your cart</p>\n          </div>\n\n          <div class=\"column is-12 box\">\n              <h2 class=\"subtitle\">Summary</h2>\n        \n              <strong>${{cartTotalPrice.toFixed(2)}}</strong>\n              <p v-if=\"cartTotalLength == 0 || cartTotalLength == 1\">{{cartTotalLength}} item</p>\n              <p v-else>{{cartTotalLength}} items</p>\n              <hr>\n              <router-link to=\"/cart/checkout\" class=\"button is-dark\">Proceed to checkout</router-link>\n          </div>\n      </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport CartItem from '../components/CartItem.vue'\n\nexport default {\n    name: 'Cart',\n    components:{\n        CartItem\n    },\n    data(){\n        return{\n            cart: {\n                items:[]\n            }\n        }\n    },\n    mounted() {\n        this.cart = this.$store.state.cart\n    },\n    methods: {\n        removeFromCart(item){\n            this.cart.items = this.cart.items.filter(i => i.product.id !== item.product.id)\n        }\n    },\n    computed:{\n        cartTotalLength(){\n            return this.cart.items.reduce((acc, curVal) => {\n                return acc += curVal.quantity\n            },0)\n        },\n        cartTotalPrice(){\n            return this.cart.items.reduce((acc, curVal) => {\n                return acc += curVal.product.price * curVal.quantity\n            },0)\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}