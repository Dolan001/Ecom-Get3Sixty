category = res.data\n                    document.title = this.category.name + ' | Djackets'\n                })\n                .catch(err => {\n                    console.log(err)\n\n                    toast({\n                        message: 'Something went wrong, try again',\n                        type: 'is-danger',\n                        dismissible: true,\n                        pauseOnHover: true,\n                        duration: 2000,\n                        position: 'top-center'\n                    })\n                })\n\n            this.$store.commit('setIsLoading', false)\n        }\n    },\n}\n</script>\n\n<style>\n\n</style>"]}]}