v class=\"home\">\n    <section class=\"hero is-medium is-dark mb-6\">\n      <div class=\"hero-body has-text-centered\">\n        <p class=\"title mb-6\">\n          Welcome to Get3sixty\n        </p>\n        <p class=\"subtitle\">\n          The best online store for dress\n        </p>\n      </div>\n    </section>\n    <div class=\"columns is-multiline\">\n\n      <div class=\"column is-12\">\n        <h2 class=\"is-size-3 has-text-centered\">Latest product</h2>\n      </div>\n\n      <div class=\"column is-3\" v-for=\"product in latestProducts\" :key=\"product.id\">\n        <div class=\"box\">\n          <figure class=\"image mb-4\">\n            <img :src=\"product.get_thumbnail\">\n          </figure>\n          <h3 class=\"is-size-4\">{{product.name}}</h3>\n          <p class=\"is-size-6 has-text-gray\">{{product.price}}</p>\n          \n          <router-link :to=\"product.get_absolute_url\" class=\"button is-dark mt-4\">View Detals</router-link>\n        </div>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'Home',\n  data(){\n    return{\n      latestProducts:[]\n    }\n  },\n  components: {\n    \n  },\n  mounted() {\n    this.getlatestProducts()\n    document.title = 'Home ' + '| Djackets'\n  },\n  methods: {\n    async getlatestProducts(){\n      this.$store.commit('setIsLoading', true)\n      await axios\n        .get('/api/v1/latest-products/')\n        .then(res =>{\n          this.latestProducts = res.data\n        })\n        .catch(err => {\n          console.log(err)\n        })\n      this.$store.commit('setIsLoading', false)  \n    }\n  },\n}\n</script>\n\n<style scoped>\n  .image{\n    margin-top: -1.25rem;\n    margin-left: -1.25rem;\n    margin-right: -1.25rem;\n  }\n</style>\n"]}]}